#!/bin/bash

cd $(dirname $0)/..  # change to the correct directory

THESIS='thesis.tex'

if [[ ! -e $THESIS ]]
then
    >&2 echo "$THESIS does not exist"
    exit 1
fi


if which latexmk > /dev/null
then
    LATEXMK=`which latexmk`
else
    >&2 echo "latexmk could not be found"
    exit 1
fi

$LATEXMK -c $THESIS
rm thesis.auxlock 
rm tikz/*
